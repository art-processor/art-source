<?xml version="1.0"?>
<!DOCTYPE x-frame SYSTEM "file:///c:\xvcl\dtd\xvcl.dtd">
<x-frame name="while" outdir="whileDir" outfile="while" language="java">
	<set var="I" value="1"/>
	<set-multi var="loop" value="?@I?, ?@I? + ?@I?,(?@I?+2),(?@I?+3)"/>
